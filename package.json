{
  "name": "field-hero-admin-server",
  "version": "1.0.0",
  "description": "Node+Express+typescript based server for the Field Hero Admin Portal.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node -r dotenv/config .",
    "lint": "eslint . --ext .ts",
    "watch": "node -r dotenv/config -r ts-node/register src/index.ts dotenv_config_path=.env.development",
    "dev": "nodemon -L"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "dependencies": {
    "@types/multer": "^1.4.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-joi-validator": "^2.0.1",
    "fs": "0.0.1-security",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "mariadb": "^2.4.2",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "pino": "^6.6.1",
    "pino-http": "^5.3.0",
    "sequelize": "^6.3.5",
    "umzug": "^2.3.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/node": "^14.11.1",
    "@types/pino": "^6.3.1",
    "@types/pino-http": "^5.0.5",
    "@types/umzug": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
